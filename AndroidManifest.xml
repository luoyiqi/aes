<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.eleonorvinicius.framentsexample"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="16" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        
        <activity
            android:name="com.eleonorvinicius.androidexamples.um.MainActivity"
            android:label="@string/app_name" >
        </activity>
        
        <activity
            android:name="com.eleonorvinicius.androidexamples.um.SplashActivity"
            android:label="@string/title_activity_splash" >
        </activity>
        
        <activity
            android:name="com.eleonorvinicius.androidexamples.dois.MenuActivity"
            android:label="@string/title_activity_menu" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        
        <activity
            android:name="com.eleonorvinicius.androidexamples.dois.ServiceActivity"
            android:label="@string/title_activity_service" >
        </activity>
        
        <activity
            android:name="com.eleonorvinicius.androidexamples.tres.SecondServiceActivity"
            android:label="@string/title_activity_service" >
        </activity>
        
        <activity
            android:name="com.eleonorvinicius.androidexamples.quatro.ThirdServiceActivity"
            android:label="@string/title_activity_service" >
        </activity>
        
        <service android:name="com.eleonorvinicius.androidexamples.dois.ServiceSample" />
        <service android:name="com.eleonorvinicius.androidexamples.tres.SecondServiceSample" />
        <service android:name="com.eleonorvinicius.androidexamples.quatro.ThirdServiceSample" />
        
        <receiver android:name="com.eleonorvinicius.androidexamples.dois.MyScheduleServiceBroadcastReceiver"/>
        <receiver android:name="com.eleonorvinicius.androidexamples.dois.MyStartServiceBroadcastReceiver"/>
    </application>
</manifest>